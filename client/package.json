{
  "name": "Jiminy",
  "repository": "https://github.com/blacklocus/jiminy",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run dev",
    "build:prod": "better-npm-run prod",
    "test": "better-npm-run test",
    "test:watch": "better-npm-ru run ",
    "lint": "better-npm-run lint",
    "lint:js": "better-npm-run lint:js",
    "lint:sass": "better-npm-run lint:sass"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node -- ./node_modules/.bin/webpack -p --config ./webpack/webpack.config.js --progress --colors"
    },
    "build": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile"
    },
    "dev": {
      "command": "nodemon --exec babel-node -- server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start ./karma.config",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "lint": {
      "command": "npm run lint:js && npm run lint:sass"
    },
    "lint:js": {
      "command": "eslint app || true"
    },
    "lint:sass": {
      "command": "sass-lint -c .sass-lint.yml 'app/**/*.scss' -v -i"
    }
  },
  "author": "Brandon Clark",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "cheerio": "^0.20.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "karma": "^1.2.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.1",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  },
  "dependencies": {
    "amcharts3": "amcharts/amcharts3",
    "amcharts3-react": "amcharts/amcharts3-react",
    "amstock3": "amcharts/amstock3",
    "autoprefixer": "^6.4.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.10",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^0.9.1",
    "html-webpack-plugin": "^2.22.0",
    "immutability-helper": "^2.0.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^0.10.1",
    "query-string": "^4.2.3",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
